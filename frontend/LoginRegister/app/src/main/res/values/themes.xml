<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.LoginRegister" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/base_color</item>
        <item name="colorPrimaryVariant">#CF340C</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#CA7735</item>
        <item name="colorSecondaryVariant">#B65A13</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor">#FF9800</item>
        <!-- Customize your theme here. -->
        <item name="android:fontFamily"> @font/manrope_regular </item>

    </style>

    <style name="Theme.LoginRegister.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.LoginRegister.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.LoginRegister.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>